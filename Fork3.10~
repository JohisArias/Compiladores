//Programa 3.10. Programa que ejecuta el mandato recibido en la l√≠nea de mandatos.
#include<sys/types.h>
#include<stdio.h>
#include<stdlib.h>
main(int argc, char **argv)
{
	pid_t pid;
	pid = fork();
	switch(pid)
	{
		case -1: /* error del fork() */
			exit(-1);
		case 0: /* proceso hijo */
			if (execvp(argv[1], &argv[1]) < 0)
				perror("exec");
		default: /* padre */
			printf ("Proceso padre %d\n", getppid());
	}//fin switch
}//fin main
